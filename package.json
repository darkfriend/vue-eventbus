{
  "name": "darkfriend-vue-eventbus",
  "version": "1.0.4",
  "description": "A Vue eventbus",
  "main": "./dist/vue-eventbus.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name vue-eventbus src/index.js",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/darkfriend/vue-eventbus.git"
  },
  "keywords": [
    "eventbus",
    "vue-eventbus",
    "bus",
    "event",
    "vue",
    "darkfriend"
  ],
  "author": "darkfriend",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darkfriend/vue-eventbus/issues"
  },
  "homepage": "https://github.com/darkfriend/vue-eventbus#readme",
  "dependencies": {
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.js",
    "*.json"
  ]
}
